<ul>
  {{#each controller}}
    <li>{{name}}
      {{#if winner}}
        <span {{bindAttr class="winner highlight" }}>winner</span>
      {{/if}}
    </li>
  {{/each}}
</ul>
{{view Ember.TextField valueBinding="newEntryName" action="addEntry"}}
<button {{action "drawWinner"}}>Draw Winner!</button>
